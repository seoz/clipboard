{
  "manifest_version": 3,
  "name": "QuickPaste",
  "version": "1.0",
  "description": "Store and manage multiple texts with easy clipboard access",
  "permissions": [
    "storage",
    "clipboardWrite"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "QuickPaste"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
